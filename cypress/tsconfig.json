{
  "compilerOptions": {
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "strict": true,
    "target": "es5",
    "lib": ["es6", "dom", "esnext"],
    "types": [
      "cypress",
      "@types/testing-library__cypress",
      "../cypress/support/index"
    ],
    "baseUrl": ".",
    "paths": {
      "types/*": ["../src/types/*"]
    }
  },
  "include": ["**/*.ts", "support/index.ts"]
}
