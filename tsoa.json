{
  "swagger": {
    "specVersion": 3,
    "entryFile": "./src/server/API/index.ts",
    "outputDirectory": "./docs",
    "host": "marudor.de",
    "basePath": "api/",
    "controllerPathGlobs": [
      "./src/server/API/controller/**",
      "./src/types/**"
    ],
    "noImplicitAdditionalProperties": "silently-remove-extras"
  },
  "routes": {
    "middleware": "koa",
    "entryFile": "./src/server/API/index.ts",
    "routesDir": "./src/server/API",
    "basePath": "api/",
    "controllerPathGlobs": [
      "./src/server/API/controller/**",
      "./src/types/**"
    ]
  }
}
