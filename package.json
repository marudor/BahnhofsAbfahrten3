  "engines": {
    "node": "^10"
  },
  "engineStrict": true,
  "scripts": {
    "build:client:perf": "npm run cleanup && NODE_ENV=production webpack --profile --json > ./webpack-stats.json && webpack-bundle-analyzer ./webpack-stats.json dist/client --mode static -s gzip --report ./BundleReport.html --no-open",
    "build:client:webpack": "npm run cleanup && webpack",
    "build:client": "npm run build:client:webpack",
    "build:server:babel": "babel src/server --no-babelrc --config-file ./.babelrc.server.js --out-dir dist/server --copy-files",
    "build:server": "npm run cleanup && npm run build:server:babel",
    "cleanup": "rimraf dist && mkdir dist",
    "dev:client": "webpack-dev-server",
    "dev:server:test": "npm run watch:server & npm run start:server:test",
    "dev:server": "npm run watch:server & npm run start:server",
    "dev:test": "npm run dev:server:test & npm run dev:client",
    "dev": "npm run dev:server & npm run dev:client",
    "lint": "eslint src",
    "start:server:test": "NODE_ENV=test npm run start:server",
    "start:server": "nodemon -w dist/server -x 'node dist/server/index.js'",
    "test": "npm run lint && npm run test:server && flow",
    "test:server": "jest --config test/server/jest.config.js",
    "watch:server": "babel src/server --no-babelrc --config-file ./.babelrc.server.js --out-dir dist/server --watch --copy-files"
  },
