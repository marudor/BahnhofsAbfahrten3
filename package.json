{
  "name": "bahnhofs-abfahrten",
  "version": "0.0.2",
  "description": "",
  "license": "MIT",
  "engines": {
    "node": "^14.0.0"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engineStrict": true,
  "scripts": {
    "all:clean": "yarn workspaces foreach -pt run build:clean",
    "all:build": "yarn workspaces foreach -p run build",
    "all:lint": "yarn workspaces foreach -pt run lint",
    "build:clean": "rimraf dist && rimraf testDist && mkdir dist && mkdir testDist",
    "build:client": "node scripts/buildClient",
    "build:client:ci": "node scripts/buildClient sendStats",
    "build:server": "node scripts/buildServer",
    "build": "yarn build:client",
    "dev": "npm run start",
    "lint": "run-p lint:eslint lint:prettier",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx scripts cypress",
    "lint:prettier": "prettier -c packages/ scripts/ cypress/",
    "start": "node devIndex.js",
    "test": "run-p lint test:jest test:ts",
    "test:jest": "jest",
    "test:ts": "run-p test:ts:main test:ts:cypress",
    "test:ts:main": "tsc",
    "test:ts:cypress": "tsc -p cypress",
    "e2e:local": "CYPRESS_baseUrl=https://local.marudor.de:9042 cypress open",
    "e2e:ci": "cypress run",
    "doc:gen": "yarn workspace server run doc:gen",
    "doc:build": "yarn workspace server run doc:build"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run doc:build && git add docs && git add packages/server/API/routes.ts"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-react-constant-elements": "^7.9.0",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@cypress/webpack-preprocessor": "^5.2.0",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@packtracker/webpack-plugin": "^2.2.0",
    "@sentry/cli": "^1.52.3",
    "@sentry/webpack-plugin": "^1.11.1",
    "@sinonjs/fake-timers": "^6.0.1",
    "@testing-library/cypress": "6.0.0",
    "@types/compression-webpack-plugin": "^2.0.1",
    "@types/jest": "^25.2.2",
    "@types/rimraf": "^3.0.0",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.41.13",
    "@types/webpack-bundle-analyzer": "2.13.3",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-plugin-jsx-remove-data-test-id": "2.1.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "cache-loader": "^4.1.0",
    "chokidar": "^3.4.0",
    "compression-webpack-plugin": "^4.0.0",
    "css-loader": "^3.5.3",
    "cypress": "^4.5.0",
    "eslint": "^6.0.0",
    "eslint-config-marudor": "^7.3.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "eslint-plugin-testing-library": "^3.1.2",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "jest-watch-typeahead": "^0.6.0",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.1",
    "typescript": "^3.9.2",
    "url-loader": "^4.1.0",
    "wait-for": "^2.1.1",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "dependencies": {
    "client": "workspace:*",
    "core-js": "^3.6.5",
    "server": "workspace:*"
  },
  "resolutions": {
    "iconv-lite": "^0.5.1",
    "node-fetch": "^2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marudor/BahnhofsAbfahrten.git"
  },
  "author": "marudor",
  "bugs": {
    "url": "https://github.com/marudor/BahnhofsAbfahrten/issues"
  },
  "keywords": [],
  "contributors": [],
  "homepage": "http://marudor.de",
  "jest-junit": {
    "output": "./reports/junitresults.xml"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "footer-leading-blank": [
        0
      ]
    }
  }
}
