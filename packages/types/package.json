{
  "name": "types",
  "version": "1.0.0",
  "author": "marudor",
  "scripts": {
    "build": "run-s build:clean build:babel",
    "build:babel": "babel -x .ts --root-mode upward . --out-dir . --ignore 'node_modules/**'",
    "build:clean": "rimraf '*[!node_modules]**/**/*.js' && rimraf '*.js'",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ."
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@types/babel__core": "7.1.14",
    "@types/eslint": "7.2.10",
    "@types/rimraf": "^3.0.0",
    "eslint": "^7.26.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "bundledDependencies": [
    "types"
  ]
}
